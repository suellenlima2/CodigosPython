a = int(input('Digite um n√∫mero para calcular a tabuada:'))

print('\nTABUADA - MULTIPLICACAO')
print(f'\n{1}*{a} = {1*10}')
print(f'{2}*{a} = {2*a}')
print(f'{3}*{a} = {3*a}')
print(f'{4}*{a} = {4*a}')
print(f'{5}*{a} = {5*a}') 
print(f'{6}*{a} = {6*a}')          
print(f'{7}*{a} = {7*a}')           
print(f'{8}*{a} = {8*a}')        
print(f'{9}*{a} = {9*a}')         
print(f'{10}*{a}= {10*a}')           
